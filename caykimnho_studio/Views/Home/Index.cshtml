<section class="product_section mb-96">
    <div class="container">
        <div class="product_header d-flex justify-content-between mb-50">
            <div class="section_title">
                <h2>SẢN PHẨM MỚI</h2>
            </div>
            <div class="product_tab_btn d-flex">
                <ul class="nav" role="tablist">
                    <li>
                        <a class="active" data-toggle="tab" href="#newproduct-all" role="tab" aria-controls="newproduct-all"
                           aria-selected="true">
                            Tất cả
                        </a>
                    </li>
                    @foreach (var item in categories)
                    {
                        <li>
                            <a data-toggle="tab" href="#newproduct-@item.ID" role="tab" aria-controls="newproduct-@item.ID"
                               aria-selected="false">
                                @item.Name
                            </a>
                        </li>
                    }
                </ul>
                <div class="all_product">
                    <a onclick="{ $('#Loaders').fadeIn('slow') }" href="@Url.Action("product", "home")">Xem thêm</a>
                </div>
            </div>
        </div>
        <div class="product_container row">
            <div class="tab-content">
                <div class="tab-pane fade show active" id="newproduct-all" role="tabpanel">
                    <div class="product_slick slick_slider_activation" data-slick='{
                            "slidesToShow": 4,
                            "slidesToScroll": 1,
                            "rows": 2,
                            "arrows": true,
                            "dots": false,
                            "autoplay": true,
                            "speed": 500,
                            "infinite": true,
                            "responsive":[
                              {"breakpoint":992, "settings": { "slidesToShow": 3 } },
                              {"breakpoint":768, "settings": { "slidesToShow": 2 } },
                              {"breakpoint":300, "settings": { "slidesToShow": 1 } }
                             ]
                        }'>
                        @foreach (var item in Model.OrderByDescending(o => o.ID).Take(8).ToList())
                        {
                            <article class="col single_product">
                                <figure>
                                    <div class="product_thumb">
                                        <a>
                                            <img class="primary_img" src="@item.Images.Split('#')[0].ToString()"
                                                 alt="consectetur">
                                        </a>
                                        @{
                                            var sale = item.DetailProducts.OrderByDescending(o => o.Sale).First().Sale;
                                            if (sale != 0)
                                            {
                                                <div class="product_label">
                                                    <span>-@((sale * 100).ToString("0,0"))%</span>
                                                </div>
                                            }
                                        }
                                    </div>
                                    <figcaption class="product_content text-center">
                                        <div class="product_ratting">
                                            <ul class="d-flex justify-content-center">
                                                <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                <li><span>(4)</span></li>
                                            </ul>
                                        </div>
                                        <h4 class="product_name">
                                            <a onclick="{ $('#Loaders').fadeIn('slow') }" href="@Url.Action("productdetail", "home", new {id = item.ID})">@item.Name</a>
                                        </h4>
                                        <div class="price_box">
                                            @{
                                                var prices = item.DetailProducts.First().Price;
                                                if (sale != 0)
                                                {
                                                    <span class="current_price">@((prices - (prices * sale)).ToString("0,0"))đ </span>
                                                    <span class="old_price"> @prices.ToString("0,0")đ</span>
                                                }
                                                else
                                                {
                                                    <span class="current_price">@prices.ToString("0,0")đ</span>
                                                }
                                            }
                                        </div>
                                        <div class="add_to_cart">
                                            <a class="btn btn-primary" id="quickViewProduct-@item.ID" name="@item.ID">Xem Nhanh</a>
                                        </div>
                                    </figcaption>
                                </figure>
                            </article>
                        }
                    </div>
                </div>
                @foreach (var items in categories)
                {
                    int ids = items.ID;
                    <div class="tab-pane fade" id="newproduct-@items.ID" role="tabpanel">
                        <div class="product_slick slick_slider_activation" data-slick='{
                            "slidesToShow": 4,
                            "slidesToScroll": 1,
                            "arrows": true,
                            "dots": false,
                            "autoplay": false,
                            "speed": 300,
                            "infinite": true,
                            "rows": 2,
                            "responsive":[
                              {"breakpoint":992, "settings": { "slidesToShow": 3 } },
                              {"breakpoint":768, "settings": { "slidesToShow": 2 } },
                              {"breakpoint":300, "settings": { "slidesToShow": 1 } }
                             ]
                        }'>
                            @foreach (var item in Model.Where(p => p.ID_Category == ids).OrderByDescending(o => o.ID).Take(8).ToList())
                            {
                                <article class="col single_product">
                                    <figure>
                                        <div class="product_thumb">
                                            <a>
                                                <img class="primary_img" src="@item.Images.Split('#')[0].ToString()"
                                                     alt="consectetur">
                                            </a>
                                            @{
                                                var sale = item.DetailProducts.OrderByDescending(o => o.Sale).First().Sale;
                                                if (sale != 0)
                                                {
                                                    <div class="product_label">
                                                        <span>-@((sale * 100).ToString("0,0"))%</span>
                                                    </div>
                                                }
                                            }
                                        </div>
                                        <figcaption class="product_content text-center">
                                            <div class="product_ratting">
                                                <ul class="d-flex justify-content-center">
                                                    <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                    <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                    <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                    <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                    <li><a href="#"><i class="ion-android-star"></i></a></li>
                                                    <li><span>(4)</span></li>
                                                </ul>
                                            </div>
                                            <h4 class="product_name">
                                                <a onclick="{ $('#Loaders').fadeIn('slow') }" href="@Url.Action("productdetail", "home", new {id = item.ID})">@item.Name</a>
                                            </h4>
                                            <div class="price_box">
                                                @{
                                                    var prices = item.DetailProducts.First().Price;
                                                    if (sale != 0)
                                                    {
                                                        <span class="current_price">@((prices - (prices * sale)).ToString("0,0"))đ </span>
                                                        <span class="old_price"> @prices.ToString("0,0")đ</span>
                                                    }
                                                    else
                                                    {
                                                        <span class="current_price">@prices.ToString("0,0")đ</span>
                                                    }
                                                }
                                            </div>
                                            <div class="add_to_cart">
                                                <a class="btn btn-primary" id="quickViewProduct-@item.ID" name="@item.ID">Xem Nhanh</a>
                                            </div>
                                        </figcaption>
                                    </figure>
                                </article>
                            }
